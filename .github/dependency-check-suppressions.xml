<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Spring Boot framework false positives -->
    <suppress>
        <notes><![CDATA[
        Spring Boot starter dependencies are managed by Spring and regularly updated
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/.*@.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- PostgreSQL JDBC driver false positives -->
    <suppress>
        <notes><![CDATA[
        PostgreSQL JDBC driver version is actively maintained
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.postgresql/postgresql@.*$</packageUrl>
        <vulnerabilityName>CVE-2020-13692</vulnerabilityName>
    </suppress>
    
    <!-- HikariCP connection pool false positives -->
    <suppress>
        <notes><![CDATA[
        HikariCP is actively maintained and security issues are promptly addressed
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.zaxxer/HikariCP@.*$</packageUrl>
        <cve>CVE-2013-4002</cve>
    </suppress>
    
    <!-- Jackson dependencies managed by Spring Boot -->
    <suppress>
        <notes><![CDATA[
        Jackson versions are managed by Spring Boot dependency management
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/.*@.*$</packageUrl>
        <cve>CVE-2017-17485</cve>
    </suppress>
    
    <!-- Test dependencies that don't affect production -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not included in production builds
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\.jupiter/.*@.*$</packageUrl>
        <cve>CVE-2020-15250</cve>
    </suppress>
    
    <!-- Suppress false positives for Java 23 preview features -->
    <suppress>
        <notes><![CDATA[
        Java 23 preview features are used with proper feature flags
        ]]></notes>
        <filePath regex="true">.*\.jar.*</filePath>
        <cve>CVE-2023-21930</cve>
    </suppress>
    
</suppressions>