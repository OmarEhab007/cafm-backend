							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    pm.environment.set('access_token', response.accessToken);",
									"    pm.environment.set('refresh_token', response.refreshToken);",
									"    pm.environment.set('user_id', response.userId);",
									"    pm.environment.set('company_id', response.companyId);",
									"    ",
									"    // Calculate token expiry",
									"    const expiryTime = new Date(Date.now() + (response.expiresIn * 1000));",
									"    pm.environment.set('token_expiry', expiryTime.toISOString());",
